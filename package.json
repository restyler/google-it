{
  "type": "module",
  "name": "google-it",
  "version": "1.6.1",
  "description": "A CLI and Node.js library to help retrieve, display, and store Google search results",
  "main": "./src/main.js",
  "bin": "./src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/PatNeedham/google-it.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint 'src/*.js'",
    "test": "jest"
  },
  "keywords": [
    "google-search",
    "google-search-to-json"
  ],
  "author": "Pat Needham (pat@needham.io)",
  "license": "ISC",
  "jest": {
    "preset": "amex-jest-preset",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.4.0",
    "command-line-args": "^5.0.0",
    "ora": "^4.0.3",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "codecov": "^3.6.2",
    "eslint": "^6.8.0",
    "eslint-config-amex": "^11.1.0",
    "jest": "^26.4.2",
    "mocha": "^8.1.3",
    "prettier": "2.0.0"
  }
}
